<script>
  $(function () {
    $('#container').highcharts({
        chart: {
          type: 'line'
        },
        title: {
          text: 'Case Quality'
        },
        xAxis: {
          type: 'date',
          dateLabelFormats: { created_at: '%m/%d/%Y'}
        },
        yAxis: {
          title: {
            text: 'Rating'
          }
        },
        series:
          {
            type: 'line',
            name: '<%= @dates %>',
            data: '<%= @all_home_reviews.each { |review| review.rating } %>'
          },

    });
  });
</script>
