<h3>Update Foster Child Information</h3>

<%= form_for @foster_kid do |f| %>
    <div class="field">
      <%= f.label :first_name, "First Name" %>
      <%= f.text_field :first_name %>
    </div>

    <div class="field">
      <%= f.label :last_name %>
      <%= f.text_field :last_name %>
    </div>

    <div class="actions">
      <%= f.submit "Update", class: "button round submit" %>
    </div>
<% end %>
